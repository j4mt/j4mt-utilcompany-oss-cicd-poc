<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="SQSTestSuite" verbose="1" parallel="methods">
    <listeners>
        <listener class-name="com.sqs.web.webdriver.WebDriverListener"/>
    </listeners>

    <test name="PrestaBestPractice" verbose="4">
        <parameter name="browser" value="Chrome"/>
        <classes>
            <class name="com.sqs.prestatests.PrestaSignInTest"/>
        </classes>
    </test>

</suite>